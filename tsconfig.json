{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Default",
  "compilerOptions": {
    "lib": [
      "ES2024",
      "DOM",
    ],
    "target": "ES2015",
    "esModuleInterop": true,
    "isolatedModules": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "strict": true,
    "incremental": true,
    "outDir": "./dist",
  },
  "exclude": [
    "node_modules"
  ]
}
